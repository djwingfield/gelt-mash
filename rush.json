{
    "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
    "rushVersion": "5.129.7",
    "npmVersion": "6.14.15",
    "nodeSupportedVersionRange": ">=18.20.3 <19.0.0 || >=20.14.0 <21.0.0",
    "nodeSupportedVersionInstructions": "Run 'nvm use' to switch to the expected node version.",
    "ensureConsistentVersions": true,
    "projectFolderMinDepth": 1,
    "projectFolderMaxDepth": 2,
    "approvedPackagesPolicy": {
        "reviewCategories": ["production", "tools", "prototypes"],
        "ignoredNpmScopes": ["@types"]
    },
    "gitPolicy": {
        "versionBumpCommitMessage": "[rush publish] Bump versions [skip ci]",
        "changeLogUpdateCommitMessage": "[rush version]Update changelogs",
        "changefilesCommitMessage": "[rush change] Rush change"
    },
    "repository": {
        "url": "https://github.com/djwingfield/rush-projects",
        "defaultBranch": "main",
        "defaultRemote": "origin"
    },
    "eventHooks": {
        "preRushInstall": [],
        "postRushInstall": [],
        "preRushBuild": [],
        "postRushBuild": [],
        "preRushx": [],
        "postRushx": []
    },
    "telemetryEnabled": true,
    "allowedProjectTags": ["app", "ui", "data", "util"],
    "projects": [
        {
            "packageName": "gelt-mash",
            "projectFolder": "apps/gelt-mash",
            "shouldPublish": false,
            "tags": ["app"],
            "reviewCategory": "production"
        }
    ]
}
